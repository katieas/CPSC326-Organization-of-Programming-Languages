#----------------------------------------------------------------------
# Nested while statement tests
#----------------------------------------------------------------------

fun void main() {
  var x = 1
  var y = 1
  var z = 1
  var c = 4

  while x < c {
    while y < c {
      while z < c {
        print(itos(x) + ", " + itos(y) + ", " + itos(z) + "\n")
        z = z + 1
      }
      y = y + 1
      z = 1
    }
    x = x + 1
    y = 1
  }

}

